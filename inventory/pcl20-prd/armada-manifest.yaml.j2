metadata:
  schema: metadata/Document/v1
  name: elasticsearch
data:
  chart_name: elasticsearch
  release: elasticsearch
  namespace: openstack
  install:
    no_hooks: false
  upgrade:
    no_hooks: false
  values:
    images:
      tags:
        apache_proxy: {{ docker_registry }}/httpd:2.4
        memory_init: {{ docker_registry }}/ubuntu-source-heat-engine:3.0.3
        curator: {{ docker_registry }}/curator:5.2.0
        elasticsearch: {{ docker_registry }}/elasticsearch-s3:v0.1.0
        ceph_key_placement: {{ docker_registry }}/ceph-config-helper:v1.10.3
        s3_bucket: {{ docker_registry }}/ceph-config-helper:v1.10.3
        s3_user: {{ docker_registry }}/ceph-config-helper:v1.10.3
        helm_tests: {{ docker_registry }}/ubuntu-source-heat-engine:3.0.3
        prometheus_elasticsearch_exporter: {{ docker_registry }}/elasticsearch_exporter:1.0.1
        dep_check: {{ docker_registry }}/kubernetes-entrypoint:v0.3.1
        snapshot_repository: {{ docker_registry }}/ubuntu-source-heat-engine:3.0.3
        image_repo_sync: {{ docker_registry }}/docker:17.07.0
      pull_policy: Always
    labels:
      elasticsearch:
        node_selector_key: elasticsearch
        node_selector_value: enabled
      job:
        node_selector_key: elasticsearch
        node_selector_value: enabled
      test:
        node_selector_key: elasticsearch
        node_selector_value: enabled
    pod:
      replicas:
        master: 3
        data: 3
        client: 2
    conf:
      curator:
        #run every 6th hour
        schedule:  "0 4 * * *"
        action_file:
          # Remember, leave a key empty if there is no value.  None will be a string,
          # not a Python "NoneType"
          #
          # Also remember that all examples have 'disable_action' set to True.  If you
          # want to use this action as a template, be sure to set this to False after
          # copying it.
          actions:
            1:
              action: delete_indices
              description: >-
                "Delete indices older than 7 days"
              options:
                timeout_override:
                continue_if_exception: False
                ignore_empty_list: True
                disable_action: False
              filters:
              - filtertype: pattern
                kind: prefix
                value: logstash-
              - filtertype: age
                source: name
                direction: older
                timestring: '%Y.%m.%d'
                unit: days
                unit_count: 31
            2:
              action: delete_indices
              description: >-
                "Delete indices older than 7 days"
              options:
                timeout_override:
                continue_if_exception: False
                ignore_empty_list: True
                disable_action: False
              filters:
              - filtertype: pattern
                kind: prefix
                value: syslog-
              - filtertype: age
                source: name
                direction: older
                timestring: '%Y.%m.%d'
                unit: days
                unit_count: 100
            3:
              action: delete_indices
              description: >-
                "Delete indices by age if available disk space is
                 less than 80% total disk"
              options:
                timeout_override: 600
                continue_if_exception: False
                ignore_empty_list: True
                disable_action: False
              filters:
              - filtertype: pattern
                kind: prefix
                value: logstash-
              - filtertype: space
                source: creation_date
                use_age: True
                # 500Gi*3*80% = 1200
                disk_space: 1200
      elasticsearch:
        env:
          java_opts: "-Xms12288m -Xmx12288m"
    monitoring:
      prometheus:
        enabled: true
    storage:
      enabled: true
      pvc:
        name: pvc-elastic
        access_mode: [ "ReadWriteOnce" ]
      requests:
        storage: 500Gi
      storage_class: rbd-lma
  source:
    type: local
    location: "{{ osh_infra_path }}"
    subpath: elasticsearch
  dependencies:
    - helm-toolkit
